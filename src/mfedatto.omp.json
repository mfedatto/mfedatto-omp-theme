{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "final_space": true,
    "version": 2,
    "console_title_template": "{{ lower .Shell }}{{ if .WSL }}::wsl{{ end }}::{{ lower .HostName }}{{ if .Root }}::su{{ end }}::{{ lower .UserName }}::{{ .Folder }}",
    "terminal_background": "#0f1920",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "#009999",
                    "properties": {
                        "time_format": "2006-01-02 15:04:05"
                    },
                    "template": "[{{ .CurrentDate | date .Format }}]"
                },
                {
                    "type": "sysinfo",
                    "style": "plain",
                    "foreground": "#6a6a6a",
                    "properties": {
                        "precision": 1
                    },
                    "template": "[CPU {{ round .PhysicalPercentUsed .Precision }}%<#808080>::</>RAM {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1068786176.0) }}GB]"
                },
                {
                    "type": "executiontime",
                    "style": "plain",
                    "foreground": "#1a2a33",
                    "properties": {
                        "style": "amarillo",
                        "threshold": 100
                    },
                    "template": " {{ .FormattedMs }} "
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "filler": "~",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#1a2a33",
                    "template": "~"
                }
            ]
        },
        {
            "type": "prompt",
            "newline": true,
            "alignment": "left",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#ffe500",
                    "template": "[{{ .PWD }}]"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "angular",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": "[angular {{ .Major }}}"
                },
                {
                    "type": "dotnet",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": "[.net {{ .Major }}}"
                },
                {
                    "type": "java",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": "[java {{ .Major }}}"
                },
                {
                    "type": "node",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": "[node {{ .Major }}}"
                },
                {
                    "type": "npm",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": "[npm {{ .Major }}}"
                },
                {
                    "type": "php",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": "[php {{ .Major }}}"
                },
                {
                    "type": "kubectl",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": "[{{ .Context }}{{ if .Namespace }}<#808080>::</>{{ .Namespace }}{{ end }}}"
                },
                {
                    "type": "az",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": "[az {{ .EnvironmentName }}{{ if not .IsDefault }} <f>not default</f>{{ end }}}"
                },
                {
                    "type": "aws",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": "[aws {{ .Profile }}{{ if .Region }}<#808080>@</>{{ .Region }}{{ end }}}"
                },
                {
                    "type": "gcp",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": "[gcp {{ .Account }}{{ if .Region }}<#808080>@</>{{ .Region }}{{ end }}<#808080>:</>{{ .Project }}}"
                },
                {
                    "type": "terraform",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": "[terraform {{ .WorkspaceName }}{{ if .Version }}<#808080>:</>{{ .Version }}{{ end }}}"
                },
                {
                    "type": "git",
                    "style": "plain",
                    "foreground": "#9c27b0",
                    "properties": {
                        "branch_icon": "",
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "template": "[{{ .HEAD }}{{ if or .Staging.Changed .Working.Changed }} {{ end }}{{ if .Staging.Changed }}{{ .Staging.String }}{{ end }}{{ if .Working.Changed }}{{ .Working.String }}{{ end }}]"
                }
            ]
        },
        {
            "type": "prompt",
            "newline": true,
            "alignment": "left",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#20ffa6",
                    "template": "{{ .UserName }}<#808080>@</>{{ if .WSL }}WSL<#808080>:</>{{ end }}{{ .HostName }}<#808080>:</>"
                },
                {
                    "type": "os",
                    "style": "plain",
                    "foreground": "#a6a6a6",
                    "properties": {
                        "macos": "mac",
                        "linux": "lnx",
                        "windows": "win",
                        "alma": "alm",
                        "alpine": "alp",
                        "aosc": "aosc",
                        "arch": "arch",
                        "centos": "cent",
                        "coreos": "core",
                        "debian": "deb",
                        "devuan": "dev",
                        "raspbian": "rpi",
                        "elementary": "elm",
                        "fedora": "fed",
                        "gentoo": "gen",
                        "mageia": "mag",
                        "manjaro": "man",
                        "mint": "mint",
                        "nixos": "nix",
                        "opensuse": "suse",
                        "redhat": "rhat",
                        "rocky": "rck",
                        "sabayon": "sby",
                        "slackware": "slck",
                        "ubuntu": "ubu",
                        "android": "and"
                    },
                    "template": "{{ .Icon }}<#808080>:</>"
                },
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#20ffa6",
                    "template": "{{ .Folder }}{{ if .Root }}<#808080>:</>{{ end }}{{ if .Root }}<f>su</f>{{ end }}<#808080>$</>"
                }
            ]
        }
    ],
    "transient_prompt": {
        "background": "transparent",
        "foreground": "#20ffa6",
        "template": "{{ .PWD }}<#808080>$</> "
    },
    "secondary_prompt": {
        "background": "transparent",
        "foreground": "#bbd9eb",
        "template": "~> "
    }
}
