{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "final_space": true,
    "version": 2,
    "console_title_template": "{{ lower .Shell }}{{ if .WSL }}::wsl{{ end }}::{{ lower .HostName }}{{ if .Root }}::su{{ end }}::{{ lower .UserName }}::{{ .Folder }}",
    "terminal_background": "#0f1920",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "#009999",
                    "properties": {
                        "time_format": "2006-01-02 15:04:05"
                    },
                    "template": "\u22b0 {{ .CurrentDate | date .Format }} \u22b1 "
                },
                {
                    "type": "sysinfo",
                    "style": "plain",
                    "foreground": "#6a6a6a",
                    "properties": {
                        "precision": 1
                    },
                    "template": "\u22b0 CPU {{ round .PhysicalPercentUsed .Precision }}% <#808080>:</> RAM {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1068786176.0) }}GB \u22b1 "
                },
                {
                    "type": "executiontime",
                    "style": "plain",
                    "foreground": "#1a2a33",
                    "properties": {
                        "style": "amarillo",
                        "threshold": 100
                    },
                    "template": "{{ .FormattedMs }} "
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "filler": "\u223b",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#1a2a33",
                    "template": "\u223b"
                }
            ]
        },
        {
            "type": "prompt",
            "newline": true,
            "alignment": "left",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#ffe500",
                    "template": "\u22b0 {{ .PWD }} \u22b1 "
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "angular",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": " \u22b0 angular {{ .Major }} \u22b1"
                },
                {
                    "type": "dotnet",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": " \u22b0 .net {{ .Major }} \u22b1"
                },
                {
                    "type": "java",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": " \u22b0 java {{ .Major }} \u22b1"
                },
                {
                    "type": "node",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": " \u22b0 node {{ .Major }} \u22b1"
                },
                {
                    "type": "npm",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": " \u22b0 npm {{ .Major }} \u22b1"
                },
                {
                    "type": "php",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": " \u22b0 php {{ .Major }} \u22b1"
                },
                {
                    "type": "kubectl",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": " \u22b0 kubectl {{.Context}}{{if .Namespace}}<#808080>::</>{{.Namespace}}{{end}} \u22b1"
                },
                {
                    "type": "az",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": " \u22b0 az {{ .EnvironmentName }}{{ if not .IsDefault }} <f>not default</f>{{ end }} \u22b1"
                },
                {
                    "type": "aws",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": " \u22b0 aws {{.Profile}}{{if .Region}}<#808080>@</>{{.Region}}{{end}} \u22b1"
                },
                {
                    "type": "gcp",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": " \u22b0 gcp {{.Account}}{{if .Region}}<#808080>@</>{{.Region}}{{end}}<#808080>:</>{{.Project}} \u22b1"
                },
                {
                    "type": "terraform",
                    "style": "plain",
                    "foreground": "#cc66ff",
                    "template": " \u22b0 terraform {{ .WorkspaceName }}{{ if .Version }}<#808080>:</>{{ .Version }}{{ end }} \u22b1"
                },
                {
                    "type": "git",
                    "style": "plain",
                    "foreground": "#9c27b0",
                    "properties": {
                        "branch_icon": "",
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "template": " \u22b0 {{ .HEAD }}{{ if or .Staging.Changed .Working.Changed }} {{ end }}{{ if .Staging.Changed }}{{ .Staging.String }}{{ end }}{{ if .Working.Changed }}{{ .Working.String }}{{ end }} \u22b1"
                }
            ]
        },
        {
            "type": "prompt",
            "newline": true,
            "alignment": "left",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#20ffa6",
                    "template": "{{ .UserName }}<#808080>@</>{{ if .WSL }}WSL<#808080>:</>{{ end }}{{ .HostName }}<#808080>:</>"
                },
                {
                    "type": "os",
                    "style": "plain",
                    "foreground": "#a6a6a6",
                    "properties": {
                        "macos": "mac",
                        "linux": "lnx",
                        "windows": "win",
                        "alma": "alm",
                        "alpine": "alp",
                        "aosc": "aosc",
                        "arch": "arch",
                        "centos": "cent",
                        "coreos": "core",
                        "debian": "deb",
                        "devuan": "dev",
                        "raspbian": "rpi",
                        "elementary": "elm",
                        "fedora": "fed",
                        "gentoo": "gen",
                        "mageia": "mag",
                        "manjaro": "man",
                        "mint": "mint",
                        "nixos": "nix",
                        "opensuse": "suse",
                        "redhat": "rhat",
                        "rocky": "rck",
                        "sabayon": "sby",
                        "slackware": "slck",
                        "ubuntu": "ubu",
                        "android": "and"
                    },
                    "template": "{{ .Icon }}<#808080>:</>"
                },
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#20ffa6",
                    "template": "{{ .Folder }}{{ if .Root }}<#808080>:</>{{ end }}{{ if .Root }}<f>su</f>{{ end }}<#808080>$</>"
                }
            ]
        }
    ],
    "transient_prompt": {
        "background": "transparent",
        "foreground": "#ffffff",
        "template": "{{ .Shell }}> "
    },
    "secondary_prompt": {
        "background": "transparent",
        "foreground": "#bbd9eb",
        "template": " \u21d2 "
    }
}
